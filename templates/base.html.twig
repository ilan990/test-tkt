    {% block body %}

        {% endblock %}

    <script>


        const menuHamburger = document.querySelector(".menu-hamburger")
        const ouverture = document.querySelector(".ouverture")
        const fermeture = document.querySelector(".fermeture")
        const navLinks = document.querySelector(".nav-links")
        navLinks.style.display =  'none';
        const menuHamburgerClose = document.querySelector(".menu-hamburger-close")

        ouverture.addEventListener('click',()=>{
        navLinks.style.display =  'flex';
        navLinks.classList.toggle('mobile-menu')

        menuHamburger.style.display =  'none' ;
        menuHamburgerClose.style.display =  'flex' ;


        })
        fermeture.addEventListener('click',()=>{
            console.log('og')
            navLinks.classList.toggle('mobile-menu')

            navLinks.style.display =  'none';
            menuHamburger.style.display =  'flex';
            menuHamburgerClose.style.display =  'none';
        })
    </script>
